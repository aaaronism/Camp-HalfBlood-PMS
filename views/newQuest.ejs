<div class="newquestmaker">
    <div class="form-group">
    <h2>Add New Quest</h2>
    <form action="/quests" method="POST">
          <label for="title">Title:</label>
          <input type="text" id="title" name="title" class="" />
          <label for="summary">Summary:</label>
          <textarea type="text" id="summary" name="summary" class=""></textarea>
          <label for="timeline">Timeline:</label>
          <input type="text" id="timeline" name="timeline" class="" />
          <label for="budget">Budget:</label>
          <input type="number" id="budget" name="budget" class="" /><br><br>
        <input type="submit" class="btn btn-primary" /><br><br>
        <a href="javascript:history.back()" class="btn btn-secondary">Cancel</a>
    </div>  
    </form>
</div>


